resource_name = "SwitchInteraction"
script/source = "extends Interact_Node

class_name Button_Interaction
var Active:bool = false

func Interact():
	var Anim:String = \"Activate\"
	if Active:
		Anim = \"Deactivate\"
	
	$PsxSwitchT1/AnimationPlayer.play(Anim,0.5)
	
	Active = !Active

"

